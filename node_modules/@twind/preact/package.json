  "repository": {
    "type": "git",
    "url": "https://github.com/tw-in-js/use-twind-with",
    "directory": "packages/preact"
  },
  "license": "MIT",
  "author": "Sascha Tandel (https://github.com/sastan)",
  "engines": {
    "node": ">=10.13"
  },
