{
  "name": "@twind/preact",
  "version": "1.0.5",
  "description": "Twind integration for preact",
  "keywords": [
    "twind",
    "preact"
  ],
  "homepage": "https://github.com/tw-in-js/use-twind-with/tree/main/packages/preact#readme",
